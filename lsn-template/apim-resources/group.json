{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "ApimServiceName": {
            "type": "string"
        }
    },
    "variables": {
    },
    "resources": [
        {
            "name": "[concat(parameters('ApimServiceName'), '/', 'group',copyIndex())]",
            "type": "Microsoft.ApiManagement/service/groups",
            "apiVersion": "2019-01-01",
            "properties": {
                "displayName": "[concat('group',copyIndex())]",
                "description": "[concat('group',copyIndex())]",
                "type": "custom",
                "externalId": ""
            },
            "copy": {
                "name": "groupsLoop",
                "count": 200
            }
        },
        {
            "name": "[concat(parameters('ApimServiceName'), '/', 'product',copyIndex(), '/','group',copyIndex())]",
            "type": "Microsoft.ApiManagement/service/products/groups",
            "apiVersion": "2019-01-01",
            "properties": {
            },
            "dependsOn": [
                "groupsLoop"
            ],
            "copy": {
                "name": "productGroupsLoop",
                "count": 200
            }
        }
    ]
}